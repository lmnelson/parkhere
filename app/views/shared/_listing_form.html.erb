<%= form_for(@listing) do |f| %>
	<%= f.label :city %>
	<%= f.text_field :city, value: "Vancouver"%>

	<%= f.label :street %>
	<%= f.text_field :street %>

	<%= f.label :description %>
	<%= f.text_area :description, rows:10 %>

	<p><%= f.submit "Add Listing", class: "btn btn-large btn-primary" %></p>
<% end %>