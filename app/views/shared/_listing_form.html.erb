<%= form_for(@listing) do |f| %>
	<%= f.label :city %>
	<%= f.text_field :city, value: "Vancouver"%>

	<label for="listing_street">Address</label>
	<%= f.text_field :street, placeholder: "1000 Example St." %>

	<label for="listing_price">Price Per Month</label>
	<%= f.text_field :price %>

	<%= f.label :description %>
	<%= f.text_area :description, rows:8 %>

	<p><%= f.submit "Add Listing", class: "btn btn-large btn-primary" %></p>
<% end %>